{
	"root": true,
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "next/core-web-vitals", "prettier"],
	"plugins": ["@typescript-eslint", "prettier"],
	"parser": "@typescript-eslint/parser",
	"rules": {
